{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.svg",
          "/**/*.cur",
          "/**/*.jpg",
          "/**/*.jpeg",
          "/**/*.png",
          "/**/*.apng",
          "/**/*.webp",
          "/**/*.avif",
          "/**/*.gif",
          "/**/*.otf",
          "/**/*.ttf",
          "/**/*.woff",
          "/**/*.woff2"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-data",
      "urls": ["https://z-backend-production-853e.up.railway.app/api/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "10s"
      }
    }
  ],
  "navigationUrls": ["/**", "!/api/**", "!/assets/**", "!/*.js", "!/*.css"]
}
